@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<QuoteModule>
@using Umbraco.Cms.Core.Models;
@using Umbraco.Cms.Web.Common.Models;
@if (Model != null)
{
    // var components = Model.Value<IEnumerable<IPublishedElement>>("components");

<section id="cta" class="parallax">
    <div class="overlay"></div>
    <div class="container">
        <div class="row text-center">
            @if (!string.IsNullOrEmpty(Model.Title))
            {
                <h2>@Model.Title</h2>
            }
            @{
                var link = Model.Value<Link>("linkToAction");
                if (link != null)
                {
                    <a href=@link.Url class="btn btn-green">@link.Name</a>
                }
            }

        </div>
    </div>
</section>
}
